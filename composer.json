{
    "name": "jn-jairo/laravel-ngrok",
    "description": "Share Laravel application with ngrok.",
    "keywords": [
        "jn-jairo",
        "laravel-ngrok"
    ],
    "homepage": "https://github.com/jn-jairo/laravel-ngrok",
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Correa",
            "email": "jn.j41r0@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/console": "^5.8|^6.0|^7.0",
        "illuminate/http": "^5.8|^6.0|^7.0",
        "illuminate/pagination": "^5.8|^6.0|^7.0",
        "illuminate/routing": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "symfony/process": "^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "JnJairo\\Laravel\\Ngrok\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JnJairo\\Laravel\\Ngrok\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit",
        "test-stable": "for version in 7.2 7.3 7.4 ; do if command -v php${version} > /dev/null 2>&1 ; then echo '\nPHP: '${version}'\n' && php${version} $(which composer) update --prefer-dist --no-interaction --prefer-stable > /dev/null 2>&1 && php${version} vendor/bin/phpunit ; fi ; done",
        "test-lowest": "for version in 7.2 7.3 ; do if command -v php${version} > /dev/null 2>&1 ; then echo '\nPHP: '${version}'\n' && php${version} $(which composer) update --prefer-dist --no-interaction --prefer-lowest --prefer-stable > /dev/null 2>&1 && php${version} vendor/bin/phpunit ; fi ; done"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JnJairo\\Laravel\\Ngrok\\NgrokServiceProvider"
            ]
        }
    }
}
